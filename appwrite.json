{
    "projectId": "jibby",
    "projectName": "",
    "functions": [
        {
            "$id": "jibby",
            "name": "jibby",
            "runtime": "node-18.0",
            "execute": [
                "any"
            ],
            "events": [],
            "schedule": "*/30 * * * *",
            "timeout": 30,
            "enabled": true,
            "logging": true,
            "entrypoint": "dist/main.js",
            "commands": "apk update && apk fetch chromium nss freetype harfbuzz ca-certificates ttf-freefont && npm install --ignore-scripts && npm run build",
            "ignore": [
                "node_modules",
                ".npm"
            ],
            "path": "functions/screenshot"
        }
    ]
}
